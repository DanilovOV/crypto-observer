{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-essential",
				"prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
			"no-tabs":"off"
    },
		"settings": {
			"import/resolver": {
				"alias": {
					"map": [
						["@", "./src"]
					],
					"extensions": [".js", ".jsx", ".ts", ".tsx", ".json"]
				}
			}
		}
}
